{
    "name": "bulk-wp/bulk-delete",
    "description": "Bulk Delete WordPress plugin",
    "license": "GPL-2.0-or-later",
    "type": "wordpress-plugin",
    "config": {
        "sort-packages": true
    },
    "repositories":[
        {
          "type": "vcs",
          "url" : "https://github.com/sudar/wp-plugin-test-tools"
